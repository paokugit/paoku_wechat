.tab-bar{
  width: 100%;
  height: 98rpx;
  background: rgb(255, 255, 255,0.95);
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  justify-content: space-around;
  align-items: center;
}
.tabImg{
  width: 45rpx;
  height: 45rpx;
  margin: 0 auto;
}
.icon_text{
  color: rgb(105, 105, 101);
  font-size: 25rpx;
}
.active{ color: rgb(193, 169, 98);}

.header_outer{
  height: 300rpx;
  background: #e8d3b8;
  position: relative;
  left: 0;
  top: 0;
}
.header_within{
  width: 90%;
  height: 236rpx;
  background: rgb(51, 55, 67);
  left: 50%;
  bottom: 0;
  transform: translate(-50%, 0);
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  padding:20rpx 0 0 40rpx;
}
.header_name{
  font-size: 34rpx;
  margin-bottom: 20rpx;
}
.header_time{font-size: 24rpx;}

.header_equity{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  left: 0;
  bottom: 0;
  background: rgb(60, 64, 76);
}
.header_text{
  font-size: 24rpx;
  color: #8c8f98;
  padding-left: 40rpx;
}
.vipTit{
  background: rgb(203, 41, 39);
  font-size: 20rpx;
  padding: 20rpx 50rpx;
  right: 0;
  top: 0;
  border-top-right-radius: 20rpx;
  border-bottom-left-radius: 80rpx;
}

.product{
  background: #ffffff;
  padding:30rpx;
}
.product_head{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.product_head_left{
  font-size: 34rpx;
  color: #333333;
}

.product_tit{
  font-size: 28rpx;
  color: #929292;
}
.product_img{
  width: 13rpx;
  height: 21rpx;
  margin:0 15rpx;
}

.product_list{
  margin:30rpx 0;
  padding: 60rpx;
  border-radius: 15rpx;
  position: relative;
  box-shadow: rgb(235, 235, 235) 0 0 30rpx;
}
.freeGet{
  width: 80rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  border-top-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
  background: #ff0509;
  left: 0rpx;
  top: 0rpx;
}
.leftImg{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.titleA{
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.titleB{
  font-size: 22rpx;
  color: #EE1515;
  margin: 10rpx 0 30rpx 0;
}

.liBao{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cardHint{
  color: rgb(2, 2, 2);
  font-size: 28rpx;
}
.btnGet{
  width: 160rpx;
  height: 50rpx;
  color: #fff;
  font-size: 35rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 50rpx;
  background: #ff0509;
}

.discountList{
  margin-top: 20rpx;
  justify-content: space-between;
  align-items: center;
}

.discountA{
  position: relative;
  left: 0;
  top: 0;
}
.discountA image{
  width: 306rpx;
  height: 134rpx;
}
.discount_details{
  left: 5%;
  top: 13%;
}
.details_money{
  font-size: 36rpx;
}
.details_money::before{
  content: "￥";
  font-size: 24rpx;
}

.ticket_a,.details_money::before{
  color: rgb(250, 42, 8);
}
.ticket_b,.details_money::before{
  color: rgb(219, 117, 52);
}

.details_tit{
  font-size: 22rpx;
}

.details_condition{
  font-size: 16rpx;
  color: rgb(212, 208, 208);
}

.getBtnA{
  width: 30rpx;
  line-height: 40rpx;
  color: #fff;
  font-size: 20rpx;
  right: 5rpx;
  bottom: 10rpx;
}

.product_area{
  width: 95%;
  margin: 15rpx auto;
  flex-wrap: wrap;
  justify-content: space-between;
}
.product_details{
  width: 350rpx;
  height: 620rpx;
  background: #fff;
  border-radius: 10rpx;
  position: relative;
  margin-bottom: 15rpx;
}
.commodity_img{
  width: 350rpx;
  height: 350rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.titWai{
  height: 90rpx;
  margin: 15rpx 15rpx 20rpx 20rpx;
}
.commodity_tit{
  color: #323232;
  font-size: 30rpx;
  display:-webkit-box; 
  overflow:hidden; 
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  line-height: 45rpx;
}
.privilege_price_tit{
  width: 90rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  background: #fff2ea;
  font-size: 26rpx;
  color: rgb(248, 87, 13);
  padding: 5rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
}
.null_hint{
  text-align: center;
  padding: 15rpx 0;
}
.hintImageA{
  width: 206rpx;
  height: 136rpx;
}
.hintImageB{
  width: 200rpx;
  height: 186rpx;
}
.hintImage{
  width: 200rpx;
  height: 182rpx;
}
.null_hint view{
  font-size: 24rpx;
  color: #929292;
}

.privilege_price_money{
  font-size: 45rpx;
  color: #FF0137;
  margin-left: 5rpx;
}
.privilege_price_money::before{
  content: "￥ ";
  font-size: 21rpx;
  color: #FF0137;
}
.original_cost{
  font-size: 26rpx;
  color: #A6A4A7;
  margin: 15rpx 0 0 20rpx;
}
.purchase_btn{
  width: 100rpx;
  height: 50rpx;
  font-size: 24rpx;
  border-radius: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background: rgb(254, 78, 78);
  right: 15rpx;
  bottom: 15rpx;
}

/* 公用css */
.txtColo{color: #ffffff;}
.locationCss{position: absolute;}
.flexCss{display: flex;}

.member_details{
  background: #fff;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 40rpx;
}
.left_message{
  align-items: center;
}
.headImg{
  width: 100rpx;
  height: 100rpx;
  margin-right: 25rpx;
  border-radius: 50rpx;
}
.my_name{
  font-size: 34rpx;
  color: #333333;
}
.expire_time,.expire_content{
  font-size: 26rpx;
  color: #929292;
}
.expire_content{
  margin-left: 15rpx;
}
.right_renew{
  width: 160rpx;
  height: 70rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 70rpx;
  color: #fff;
  border-radius: 50rpx;
  background: linear-gradient(to right, #d2b26a , #c19443);
}

.equities_list{
  width: 96%;
  margin: 20rpx auto;
  background: #fff;
  border-radius: 10rpx;
}

.contentTitle{
  text-align: center;
  padding-top: 40rpx;
}
.content_titA{
  font-size: 40rpx;
  color: #333333;
  margin-bottom: 10rpx;
}
.content_titB{
  font-size: 24rpx;
  color: #b1b1b1;
}

.details_list{
  justify-content: space-around;
  align-items: center;
  margin-top: 60rpx;
  padding-bottom: 45rpx;
}
.gift_bagImg{
  width: 70rpx;
  height: 70rpx;
  margin: 10rpx auto;
}
.gift_bagContent{
  font-size: 24rpx;
  color: #333333;
}

.get_site{
  background: #fff;
  height: 200rpx;
  line-height: 100rpx;
}
.record{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid rgb(241, 241, 241);
  margin-left: 30rpx;
}
.record_left{
  color: #333333;
  font-size: 34rpx;
}
.recordImg{
  width: 60rpx;
  text-align: center;
}
.record_img{
  width: 13rpx;
  height: 21rpx;
  margin-right: 35rpx;
}