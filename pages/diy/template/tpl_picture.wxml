<!-- 商城1 -->
<view class="fui-picture" style="background:{{diyitem.style.background}}">
    <block wx:for="{{diyitem.data}}" wx:for-index="childid" wx:for-item="childitem" wx:key="index">
        <view style="padding:{{diyitem.style.paddingtop==0?0:diyitem.style.paddingtop+'rpx'}} {{diyitem.style.paddingleft==0?0:diyitem.style.paddingleft+'rpx'}}">
            <image mode="widthFix" src="{{childitem.imgurl}}" style="{{bannerheight?'height:'+bannerheight+'px':'height:auto'}}"></image>
        </view>
    </block>
</view>
