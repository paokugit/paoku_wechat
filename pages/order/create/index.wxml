<view class="{{isIpx?'fui-iphonex-button':''}}"></view>
<block>
    <block wx:if="{{!show}}">
        <loading>加载中...</loading>
    </block>
</block>
<block>
    <block wx:if="{{show}}">
        <navigation id='Navigation' show-icon='{{showIcon}}' title="确认订单"></navigation>
        <view class="page footer {{paddingb}}">
            <block>
                <block wx:if="{{list.showTab&&!list.isonlyverifygoods}}">
                    <view class="nav">
                        <view bindtap="dispatchtype" class="{{data.dispatchtype==0?'red':'default'}}" data-type="0">快递配送</view>
                        <block>
                            <block wx:if="{{list.iscycelbuy!=1}}">
                                <view bindtap="dispatchtype" class="{{data.dispatchtype==1?'red':'default'}}" data-type="1">上门自提</view>
                            </block>
                        </block>
                    </view>
                </block>
            </block>
            <view>
                <block>
                    <block wx:if="{{list.showAddress&&!list.isonlyverifygoods}}">
                        <block>
                            <block wx:if="{{data.dispatchtype==0}}">
                                <view class="fui-cell-group" id="addressInfo">
                                    <block>
                                        <block wx:if="{{list.address}}">
                                            <navigator class="fui-cell" hoverClass="none" openType="navigate" style="padding-top:32rpx;padding-bottom:32rpx" url="/pages/member/address/select">
                                                <block>
                                                    <block wx:if="{{list.address}}">
                                                        <view class="fui-cell-icon" style="margin-right:24rpx">
                                                            <text class="icox icox-dingwei1"></text>
                                                        </view>
                                                    </block>
                                                </block>
                                                <view class="fui-cell-text textl info" style="{{list.address?'':'display:flex;justify-content: center;'}}">
                                                    <block>
                                                        <block wx:if="{{!list.address}}">
                                                            <view class="fui-cell-icon">
                                                                <text class="icox icox-icon02" style="color:#ff5555"></text>
                                                            </view>
                                                        </block>
                                                    </block>
                                                    <block>
                                                        <block wx:if="{{list.address}}">
                                                            <view class="has-address ">
                                                                <view>收货人：</view>
                                                                <text class="name">{{list.address.realname}} </text>
                                                                <text class="mobile">{{list.address.mobile}}</text>
                                                            </view>
                                                        </block>
                                                    </block>
                                                    <block>
                                                        <block wx:if="{{list.address}}">
                                                            <view class="adress">{{list.address.province+list.address.city+list.address.area+' '+list.address.address}}</view>
                                                        </block>
                                                    </block>
                                                    <block>
                                                        <block wx:if="{{!list.address}}">
                                                            <view bindtap="address" class="text no-address" data-type="open">添加收货地址</view>
                                                        </block>
                                                    </block>
                                                </view>
                                                <view class="fui-cell-remark"></view>
                                            </navigator>
                                        </block>
                                    </block>
                                    <block>
                                        <block wx:if="{{!list.address&&!list.isonlyverifygoods}}">
                                            <navigator class="fui-cell" hoverClass="none" openType="navigate" style="padding-top:32rpx;padding-bottom:32rpx" url="/pages/member/address/post?type=quickaddress">
                                                <view class="fui-cell-text textl info" style="display:flex;justify-content: center;">
                                                    <view class="fui-cell-icon">
                                                        <text class="icox icox-icon02" style="color:#ff5555"></text>
                                                    </view>
                                                    <view class="text no-address" data-type="open">添加收货地址</view>
                                                </view>
                                                <view class="fui-cell-remark"></view>
                                            </navigator>
                                        </block>
                                    </block>
                                </view>
                            </block>
                        </block>
                        <block>
                            <block wx:if="{{list.iscycelbuy}}">
                                <view class="fui-cell-group">
                                    <view class="fui-cell">
                                        <view class="fui-cell-icon">
                                            <i class="icox icox-xiangmuzhouqi" style="color: #ff6a54;"></i>
                                        </view>
                                        <view class="fui-cell-info" style="color: #333;">周期购</view>
                                    </view>
                                    <view class="fui-cycle">
                                        <view bindtap="editdate" class="fui-cycle-left" style="background-color: #ffe8df;">
                                            <view class="text">预计送达时间</view>
                                            <view class="date">
                                                <span class="predicttime" data-date="">{{currentDate}}</span>
                                                <i class="icox icox-xiugai select-showtime" style="font-size: 24rpx; margin-left:4rpx"></i>
                                            </view>
                                        </view>
                                        <view class="fui-cycle-right">
                                            <view class="text">套餐内容</view>
                                            <view class="date">
                                                <span class="cycel_day">{{list.cycelComboDay}}</span>
                                                <block>
                                                    <block wx:if="{{list.cycelComboUnit=='0'}}">
                                                        <span>天</span>
                                                    </block>
                                                </block>
                                                <block>
                                                    <block wx:if="{{list.cycelComboUnit=='1'}}">
                                                        <span>周</span>
                                                    </block>
                                                </block>
                                                <block>
                                                    <block wx:if="{{list.cycelComboUnit=='2'}}">
                                                        <span>月</span>
                                                    </block>
                                                </block>1期，共
                                                <span class="cycel_num">{{list.cycelComboPeriods}}</span>期</view>
                                        </view>
                                    </view>
                                </view>
                            </block>
                        </block>
                        <block>
                            <block wx:if="{{data.dispatchtype==1}}">
                                <view class="fui-cell-group">
                                    <navigator class="fui-cell" hoverClass="none" openType="navigate" style="padding-top:16px;padding-bottom:16px" url="/pages/order/store/index?merchid={{merchid}}">
                                        <view class="fui-cell-icon" style="margin-right:24rpx">
                                            <text class="icox icox-dianpu1"></text>
                                        </view>
                                        <view class="fui-cell-text textl info">
                                            <view class="shop_name">{{list.carrierInfo.storename}}</view>
                                            <view class="adress">{{list.carrierInfo.address}}</view>
                                        </view>
                                        <view class="fui-cell-remark"></view>
                                    </navigator>
                                </view>
                            </block>
                        </block>
                    </block>
                </block>
                <block>
                    <block wx:if="{{(list.set_mobile==0||list.set_realname==0)&&(!list.showAddress||data.dispatchtype==1)}}">
                        <view class="fui-cell-group">
                            <block>
                                <block wx:if="{{list.set_realname==0}}">
                                    <view class="fui-cell">
                                        <view class="fui-cell-label">联系人</view>
                                        <view class="fui-cell-info">
                                            <input bindinput="listChange" class="fui-input textl" id="realname" placeholder="请输入联系人" type="text" value="{{list.member.realname}}"></input>
                                        </view>
                                    </view>
                                </block>
                            </block>
                            <block>
                                <block wx:if="{{list.set_mobile==0}}">
                                    <view class="fui-cell">
                                        <view class="fui-cell-label">联系方式</view>
                                        <view class="fui-cell-info">
                                            <input bindinput="listChange" class="fui-input textl" id="mobile" placeholder="请输入联系方式" type="text" value="{{list.member.mobile}}"></input>
                                        </view>
                                    </view>
                                </block>
                            </block>
                        </view>
                    </block>
                </block>
                <block>
                    <block wx:if="{{list.isforceverifystore==1&&list.isverify==1}}">
                        <view class="fui-cell-group">
                            <navigator class="fui-cell" hoverClass="none" openType="navigate" style="padding-top:16px;padding-bottom:16px" url="/pages/order/store/index?merchid={{merchid}}&ids={{list.storeids}}&type=2">
                                <view class="fui-cell-icon" style="margin-right:24rpx">
                                    <text class="icox icox-dianpu1"></text>
                                </view>
                                <block>
                                    <block wx:if="{{!list.storeInfo}}">
                                        <text>请选择门店</text>
                                    </block>
                                </block>
                                <view class="fui-cell-text textl info">
                                    <view class="shop_name">{{list.storeInfo.storename}}</view>
                                    <view class="adress">{{list.storeInfo.address}}</view>
                                </view>
                                <view class="fui-cell-remark"></view>
                            </navigator>
                        </view>
                    </block>
                </block>
                <block>
                    <block wx:if="{{city_express_state}}">
                        <view bindtap="sendclick" class="fui-cell-group">
                            <view class="fui-cell">
                                <view class="fui-cell-label" style="width: 50rpx;">
                                    <i class="icox icox-huoche" style="color: #21ca4d;"></i>
                                </view>
                                <view class="fui-cell-info">本单支持同城配送</view>
                                <view class="fui-cell-remark"></view>
                            </view>
                        </view>
                    </block>
                </block>
                <block wx:for="{{list.goods}}">
                    <view class="fui-list-group">
                        <navigator class="fui-list-group-title" hoverClass="none" openType="switchTab" url="/pages/index/index">
                            <view class="fui-cell-icon" style="margin-right:12px">
                                <text class="icox icox-dianpu1"></text>
                            </view>
                            <text class="shop">{{item.shopname}}</text>
                        </navigator>
                        <block wx:for="{{item.goods}}" wx:for-index="key" wx:for-item="val">
                            <view class="fui-list goods-item noclick">
                                <view bindtap="url" class="fui-list-media" data-url="/pages/goods/detail/index?id={{val.goodsid}}">
                                    <image class=" goods_img" src="{{val.thumb}}"></image>
                                </view>
                                <view bindtap="url" class="fui-list-inner" style="position:relative" data-url="/pages/goods/detail/index?id={{val.goodsid}}">
                                    <view class="text">
                                        <block>
                                            <block wx:if="{{val.hasdiscount}}">
                                                <text class="fui-label fui-label-danger">折扣</text>
                                            </block>
                                        </block>{{val.title}}</view>
                                    <block>
                                        <block wx:if="{{val.optionid!=0}}">
                                            <view class="subtitle cart-option">
                                                {{val.optiontitle}}
                                            </view>
                                        </block>
                                    </block>
                                    <!-- <block>
                                        <block wx:if="{{val.is_remote==0}}">
                                            <view class="support-text" >偏远地区不支持发货</view>
                                        </block>
                                    </block> -->
                                    
                                </view>
                                <view class="fui-list-angle">
                                    <block>
                                        <block wx:if="{{list.isdiscountprice>0}}">
                                            <view class="fui-cell text-danger">￥{{val.promotionprice}}</view>
                                        </block>
                                    </block>
                                    <text class="price" style="text-decoration:{{list.isdiscountprice>0?'line-through':''}}">¥{{val.price}}</text>
                                    <block>
                                        <block wx:if="{{list.changenum&&list.iscycelbuy!=true&&bargainid<=0}}">
                                            <view bindtap="number" class="fui-number small" data-id="{{val.id}}" data-index="{{index}}" data-key="{{key}}" data-max="{{val.totalmaxbuy}}" data-min="{{val.minbuy}}" data-value="{{val.total}}">
                                                <view class="minus {{val.total<=1?'disabled':''}}" data-action="minus">-</view>
                                                <input bindblur="inputNumber" class="num shownum" name="" type="tel" value="{{val.total}}"></input>
                                                <view class="plus {{val.total>=val.totalmaxbuy?'disabled':''}}" data-action="plus">+</view>
                                            </view>
                                        </block>
                                        <block wx:elif="{{!list.changenum||list.iscycelbuy==true||bargainid>0}}">
                                            <view class="text-right">
                                                x{{val.total}}
                                            </view>
                                        </block>
                                    </block>
                                </view>
                            </view>
                        </block>

                        <block>
                            <block wx:if="{{list.giftid>0}}">
                                <view class="fui-list no-border" style="background: #f9f9f9;margin-top: 4rpx">
                                    <block wx:for="{{list.gift}}" wx:for-item="val">
                                        <block>
                                            <block wx:if="{{index<5}}">
                                                <view class="fui-list-media">
                                                    <image src="{{val.thumb}}" style="width:100rpx;height:100rpx;"></image>
                                                </view>
                                            </block>
                                        </block>
                                    </block>
                                    <view class="fui-list-inner"></view>
                                    <view class="row-remark">
                                        <view>
                                            <text>共{{list.gift.length}}件</text>
                                        </view>
                                    </view>
                                </view>
                            </block>
                        </block>
                    </view>
                </block>
                <view style='display:{{inviteDis}}'>
                    <view class='inviter-box'>
                        <view class='inviter-left'>
                            <view class='invietr-img'>
                                <image src='{{ inviteavatar}}'></image>
                            </view>
                            <view class='inviter-text'>邀请人({{invitename}})</view>
                        </view>
                        <view class='inviter-right'>{{ invitemobile}}</view>
                    </view>
                </view>
                <block>
                    <block wx:if="{{list.giftid==0&&list.gifts}}">
                        <view bindtap="giftPicker" class="fui-cell-group fui-sale-group">
                            <view class="fui-cell">
                                <view class="fui-cell-label">赠品</view>
                                <block>
                                    <block wx:if="{{list.gifts.length==1}}">
                                        <view class="fui-cell-info">{{list.gifts[0].title}}</view>
                                    </block>
                                    <block wx:else>
                                        <view class="fui-cell-info" style="{{gift_title?'':'color:#666;'}}">{{gift_title?gift_title:'请选择赠品'}}</view>
                                    </block>
                                </block>
                                <view class="fui-cell-remark"></view>
                            </view>
                        </view>
                    </block>
                </block>
                <view class="fui-cell-group bordertop" style="margin-top: 0;">
                    <block>
                        <block wx:if="{{list.fullbackgoods!=''&&list.fullbackgoods!=undefined}}">
                            <view class="fui-cell">
                                <view class="fui-cell-label">{{list.fullbacktext}}详情</view>
                                <view class="fui-cell-info">
                                    <i class="icox icox-xiaofei1" style="color: #ff5555;position: relative;top: 4rpx;margin-right: 5rpx;"></i> 总金额
                                    <i style="color: #ff5555;">
                                        <block>
                                            <block wx:if="{{list.fullbackgoods.type==0}}">￥{{list.fullbackgoods.minallfullbackallprice}}</block>
                                        </block>
                                        <block>
                                            <block wx:if="{{list.fullbackgoods.type==1}}">{{list.fullbackgoods.minallfullbackallratio}}%</block>
                                        </block>
                                    </i> ,每天返
                                    <i style="color: #ff5555;">
                                        <block>
                                            <block wx:if="{{list.fullbackgoods.type==0}}">￥{{list.fullbackgoods.fullbackprice}}</block>
                                        </block>
                                        <block>
                                            <block wx:if="{{list.fullbackgoods.type==1}}">{{list.fullbackgoods.fullbackratio}}%</block>
                                        </block>
                                    </i> ，时间：
                                    <i style="color: #ff5555;">{{list.fullbackgoods.day}}天</i>
                                </view>
                            </view>
                        </block>
                    </block>
                </view>
                <block>
                    <block wx:if="{{hasinvoice}}">
                        <view class="fui-list-group">
                            <view class="fui-cell-group">
                                <view class="fui-cell">
                                    <view class="fui-cell-label">发票抬头</view>
                                    <view class="fui-cell-info">
                                        <input bindinput="listChange" bindtap="showinvoicepicker" class="fui-input" disabled="true" id="invoicename" name="" type="text" value="{{list.invoicename}}"></input>
                                    </view>
                                </view>
                                <view class="fui-cell">
                                    <view class="fui-cell-info" style="text-align: right;">共
                                        <text class="text-danger">{{list.total}}</text> 件商品 合计：
                                        <text class="text-danger">¥ {{list.goodsprice}}</text>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </block>
                </block>
                <block>
                    <block wx:if="{{invoicepicker}}">
                        <view class="fui-mask" style="display:block"></view>
                    </block>
                </block>
                <view class="{{invoicepicker?'invoice-picker':'invoice-picker invoice-pickernone'}}">
                    <view bindtap="noinvoicepicker" style="flex: 1;"></view>
                    <view class="whiteinvoice">
                        <view class="invoicetit">发票材质
                            <text bindtap="clearinvoice" style="float: right;text-align: right;padding-left:50rpx;">清空</text>
                        </view>
                        <view class="flex invoice-title">
                            <block>
                                <block wx:if="{{list.invoice_type!=0}}">
                                    <view bindtap="chaninvoice" class="{{list.invoice_info.entity?'':'active'}}" data-type="0">电子普通发票</view>
                                </block>
                            </block>
                            <block>
                                <block wx:if="{{list.invoice_type!=1}}">
                                    <view bindtap="chaninvoice" class="{{list.invoice_info.entity?'active':''}}" data-type="1">纸质普通发票</view>
                                </block>
                            </block>
                        </view>
                        <view class="invoicetit">发票类型</view>
                        <view class="flex invoice-title">
                            <view bindtap="changeType" class="{{list.invoice_info.company?'':'active'}}" data-type="0">个人</view>
                            <view bindtap="changeType" class="{{list.invoice_info.company?'active':''}}" data-type="1">单位</view>
                        </view>
                        <view class="flex">
                            <text class="invoicesubtit">发票抬头：</text>
                            <input bindblur="invoicetitle" type="text" value="{{list.invoice_info.title?list.invoice_info.title:''}}"></input>
                        </view>
                        <block>
                            <block wx:if="{{list.invoice_info.company}}">
                                <view class="flex">
                                    <text class="invoicesubtit">纳税人识别号：</text>
                                    <input bindblur="invoicenumber" type="text" value="{{list.invoice_info.number?list.invoice_info.number:''}}"></input>
                                </view>
                            </block>
                        </block>
                        <view bindtap="confirminvoice" class="btn btn-danger">确定</view>
                    </view>
                </view>
                <view class="fui-cell-group">
                    <view class="fui-cell fui-cell-textarea">
                        <view class="fui-cell-label">
                            买家留言
                        </view>
                        <view class="fui-cell-info">
                            <block>
                                <block wx:if="{{!cycledate&&!invoicepicker}}">
                                    <textarea bindinput="dataChange" id="remark" placeholder="50字以内（选填）" rows="2"></textarea>
                                </block>
                            </block>
                        </view>
                    </view>
                </view>
                <block>
                    <block wx:if="{{diyform.fields!=null&&diyform.fields.length>0}}">
                        <view class="fui-cell-group clearform">
                            <view bindtap="clearform" class="fui-cell">
                                <view class="fui-cell-info">表单</view>
                                <view class="fui-cell-remark noremark" style="color:#999">
                                    <text class="icox icox-delete" style="font-size:26rpx"></text> 清空</view>
                            </view>
                        </view>
                    </block>
                </block>
                <include src="/pages/common/diyform.wxml"></include>
                <include src="/pages/common/city-picker.wxml"></include>
                <view class="fui-mask {{showPicker||gift?'show':''}}"></view>
                <include src="/pages/common/gift-picker.wxml"></include>
                <view class="fui-cell-group">
                    <block>
                        <block wx:if="{{list.couponcount>0}}">
                            <navigator class="fui-cell" hoverClass="none" openType="navigate" url="/pages/public/coupon/index?type=0">
                                <view class="fui-cell-info" style="color:#666">{{!data.couponname?'选择优惠券':'已选择优惠券：'+data.couponname}}</view>
                                <view class="fui-cell-remark">
                                    <view class="badge badge-danger">{{list.couponcount}}</view>
                                </view>
                                <block>
                                    <block wx:if="{{list.couponcount<=0}}">
                                        <text class="text">无可用</text>
                                    </block>
                                </block>
                            </navigator>
                        </block>
                    </block>

                    <block>
                        <block wx:if="{{list.deductcredit>0}}">
                            <view style="display:{{kllDis}}">
                                <view class="fui-cell">
                                    <text class="text-danger">{{list.deductcredit}}</text>
                                    <view class="fui-cell-label" style="text-align:left;width:auto">卡路里可抵扣
                                        <text class="text-danger">{{list.deductmoney}}</text> 元
                                    </view>
                                    <view class="fui-cell-info">
                                        <switch bindchange="dataChange" class="switch-80 pull-right" id="deduct"></switch>
                                    </view>
                                </view>
                            </view>
                        </block>
                    </block>
                    <!-- 折扣宝 -->
                    <block>
                        <block wx:if="{{list.discount>0}}">
                            <view style="display:{{zkbDis}}">
                                <view class="fui-cell">
                                    <text class="text-danger">{{list.discount}}</text>
                                    <view class="fui-cell-label" style="text-align:left;width:auto">折扣宝可抵扣
                                        <text class="text-danger">{{list.discount}}</text> 元
                                    </view>
                                    <view class="fui-cell-info">
                                        <switch bindchange="dataChange" class="switch-80 pull-right" id="discount"></switch>
                                    </view>
                                </view>
                            </view>
                        </block>
                    </block>


                    <block>
                        <block wx:if="{{list.deductcredit2>0}}">
                            <view class="fui-cell">
                                <view class="fui-cell-label" style="text-align:left;width:auto">余额可抵扣
                                    <text class="text-danger">{{list.deductcredit2}}</text> 元
                                </view>
                                <view class="fui-cell-info">
                                    <switch bindchange="dataChange" class="switch-80 pull-right" id="deduct2"></switch>
                                </view>
                            </view>
                        </block>
                    </block>
                    <!-- 卡路里 -->
                    <view class='calorieBox' style='display:{{caloriedisp}}'>
                        <view class='calorie-box' bindtap='caloriebtn'>
                            <view class='calorie-left'>卡路里还差
                                <text class='calorie-count'>{{calorienum}}</text>,不能享受免费兑,快去挣更多哦</view>
                            <view class='ckgd-img'>
                                <image src='https://www.paokucoin.com/img/backgroup/chakangengduo.png'></image>
                            </view>
                        </view>
                    </view>
                    <!-- 卡路里 -->
                </view>
                <block>
                    <block wx:if="{{list.stores.length>0&&list.isforceverifystore!=1}}">
                        <view class="fui-cell-group {{toggleStore?'toggleSend-group':''}}">
                            <view bindtap="toggle" class="fui-cell" data-id="{{toggleStore}}" data-type="toggleStore">
                                <view class="fui-cell-icon">
                                    <text class="icox icox-dianpu1"></text>
                                </view>
                                <view class="fui-cell-label differ">
                                    <text class="store">适用门店</text>
                                </view>
                                <view class="fui-cell-info"></view>
                                <view class="num">{{stores.length}}</view>
                                <view class="fui-cell-remark"></view>
                            </view>
                            <view class="send-code">
                                <block wx:for="{{list.stores}}">
                                    <view class="fui-list noclick">
                                        <view class="fui-list-media">
                                            <text class="icox icox-dianpu1"></text>
                                        </view>
                                        <view class="fui-list-inner store-inner">
                                            <view class="title">
                                                <text class="storename">{{item.storename}}</text>
                                            </view>
