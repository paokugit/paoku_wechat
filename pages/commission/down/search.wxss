.fandetails_all {
  height: 100%;
  background: #fff;
}

.header_search {
  display: flex;
  align-items: center;
}

.header_search image {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  left: 10%;
  bottom: 0;
}

.header_search input {
  width: 60%;
  margin: 0 30rpx 0 10rpx;
  padding-left: 80rpx;
  background: rgb(248, 248, 248);
}

.header_search view {
  width: 100rpx;
  text-align: center;
  line-height: 88rpx;
  background: #01d7a1;
  color: #fff;
}

.header_css {
  height: 88rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
}

