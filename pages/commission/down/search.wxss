.list_result{
  font-size: 30rpx;
  color: #9a9a9a;
  margin: 60rpx 0;
  text-align: center;
}
.friend_tier{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 35rpx;
  padding: 0 35rpx 40rpx 0;
  border-bottom: 1px solid rgb(241, 241, 241);
}

.list_left{
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.fui-list-media {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.name_vip{
  display: flex;
  align-items: center;
}
.friend-nickname {
  width: 65px;
  height: 40rpx;
  line-height: 40rpx;
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.img_box {
  width: 28rpx;
  height: 28rpx;
  vertical-align: top;
}

.friend_vip {
  color: #9a9a9a;
  font-size: 24rpx;
  margin-left: 15rpx;
}

.subtitle {
  font-size: 24rpx;
  color: #999;
}

.recommend{
  font-size: 24rpx;
  color: #333333;
}
.recommend_name{
  font-size: 28rpx;
  color: #9a9a9a;
}