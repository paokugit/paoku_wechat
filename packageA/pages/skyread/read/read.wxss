image{
  width: 100%;
  height: 100%;
}
.expedite-box{
  padding: 5rpx 24rpx;
}

.listcss{
  background: #fff;
  border-radius: 15rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx;
  margin: 20rpx 0;
}
.textcss{
  font-size: 34rpx;
  color: #333333;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3; 
  -webkit-box-orient: vertical;
}
.titimg{
  width: 190rpx;
  height: 190rpx;
  border-radius: 15rpx;
}

.finish_inquire{
  font-size: 28rpx;
  color: #8A8A8A;
  padding: 60rpx 0;
  text-align: center;
}