page {
    width: 100%;
    height: 100%;
    background: #f1f1f1;
    overflow-x: hidden;
}

.setpw-box {
    width: 100%;
    height: 100%;
}

.setpw-con {
    width: 100%;
    height: auto;
    padding-top: 26rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.setbox, .confirmbox {
    width: 620rpx;
    height: 100rpx;
    padding: 0 35rpx;
    background: #fff;
    border-radius: 8rpx;
    display: flex;
    align-items: center
}
.set-text{
    color: #333;
    font-size: 34rpx;
    margin-right: 20rpx;
}
.confirmbox{
    margin-top: 26rpx
}
.complete{
    width: 690rpx;
    height: 88rpx;
    background: #01d7a1;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    font-size: 34rpx;
    margin-top: 70rpx;
    border-radius: 8rpx;
}