page{
    width: 100%;
    height: 100%;
    background: #f8f8f8;
    overflow-x: hidden;
}
.set-box{
    width: 100%;
    height: 100%;
}
.set-con{
    width: 100%;
    height: 150rpx;
    display: flex;
    justify-content: center;
    align-items: center
}
.set-con-center{
    width: 620rpx;
    height: 100rpx;
    padding: 0 35rpx;
    background: #fff;
    border-radius: 5rpx;
    display: flex;
    justify-content: space-between;
    align-items: center
}
.set-text{
    color: #333;
    font-size: 34rpx
}
.set-img{
    width: 16rpx;
    height: 28rpx;
    display: flex;
    justify-content: center;
    align-items: center
}
image{
    width: 100%;
    height: 100%
}