.shop-name {
  font-size: 34rpx;
  color: #333;
  margin-left: 20rpx;
}

.goods-logo {
  width: 200rpx;
  height: 200rpx;
  background: red;
  margin-right: 24rpx;
}

.goods-name {
  color: #333;
  font-size: 34rpx;
}

.goods-standard {
  color: #999;
  font-size: 24rpx;
}

.goods-cost {
  font-size: 35rpx;
  color: #01d7a1;
}

.goods-cost::before {
  content: "￥";
  color: #01d7a1;
  font-size: 24rpx;
}

.goods-num {
  font-size: 28rpx;
  color: #333;
}

.goods-support {
  font-size: 18rpx;
  border: 1px solid #01d7a1;
  color: #01d7a1;
  margin-top: 10rpx;
  border-radius: 5rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding-left: 10rpx;
}

.goods-delivery {
  font-size: 28rpx;
  color: #333;
  justify-content: space-between;
  margin: 30rpx 0;
}

.fui-cell-label {
  display: block;
  width: 160rpx;
  padding: 0;
  font-size: 28rpx;
  color: #666;
  height: 100rpx;
}

.fui-cell-info {
  flex: 1;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 28rpx;
  color: #000;
}

.fui-cell-info textarea {
  width: 100%;
  height: auto;
  min-height: 100rpx;
  background: transparent;
  border: none;
  resize: none;
  font-size: 28rpx;
}

.catalog{
  padding: 30rpx 26rpx;
  border-radius: 20rpx;
  background: #fff;
  margin-bottom: 130px;
}

.goods-pay,.carriage{
  justify-content: space-between;
  margin: 15rpx 0;
}

.goods-tetx{
  font-size: 28rpx;
  color: #333;
}
.goods-money{
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
}


.go-payment{
  width: 100%;
  height: 120rpx;
  background: #fff;
  justify-content: space-between;
  position: fixed;
  left: 0;
  bottom: 0;
}
.reality-pay{
  width: 530rpx;
  height: 100%;
  line-height: 120rpx;
  padding-left: 24rpx;
}

.reality-pay-tetx{
  font-size: 30rpx;
  color: #333;
  margin-right: 20rpx;
}

.reality-pay-num{
  font-size: 40rpx;
  color: #01D7A1;
}

.reality-pay-num::before{
  content: "￥";
  font-size: 30rpx;
  color: #01D7A1;
}

.btn-pay{
  width: 220rpx;
  height: 100%;
  font-size: 30rpx;
  color: #fff;
  background: #01D7A1;
  text-align: center;
  line-height: 120rpx;
}