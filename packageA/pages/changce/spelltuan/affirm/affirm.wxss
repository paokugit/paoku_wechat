.flexcss {
  display: flex;
  align-items: center;
}
image{
  width: 100%;
  height: 100%;
}

.spillcss {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.entirety {
  padding: 20rpx 24rpx 150px;
}

.address ,.addsite{
  border-radius: 20rpx;
  background: #fff;
  padding: 24rpx;
}
.addsite{
  height: 120rpx;
  justify-content: space-between;
}
.addsite-txt{
  font-size: 36rpx;
  color: #333;
  margin-left: 30rpx;
}

.location-tit {
  background: #01d7a1;
  padding: 4rpx 6rpx;
  border-radius: 4rpx;
  color: #fff;
  font-size: 20rpx;
  margin-right: 10rpx;
}

.location-detail {
  width: 220px;
  font-size: 26rpx;
  color: #333;
}

.particular-ip {
  width: 300px;
  color: #333;
  font-size: 34rpx;
}

.standard-img {
  width: 15rpx;
  height: 24rpx;
}

.ip-name{
  
}

.ip-name, .ip-number {
  font-size: 26rpx;
  color: #333;
  margin-right: 20rpx;
}

.goods-detail {
  border-radius: 20rpx;
  background: #fff;
  padding: 30rpx 24rpx;
  margin: 20rpx 0;
}

