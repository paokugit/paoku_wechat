.flexcss{
  display: flex;
  align-items: center;
}

image{
  width: 100%;
  height: 100%;
}

.spell-list{
  justify-content: space-between;
  padding: 26rpx 24rpx;
  background: #fff;
  border-bottom: 1px solid #F0F0F0;
}

.portrait-img{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.soell-name{
  color: #333333;
  font-size: 24rpx;
  margin-left: 22rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.people-time-tetx{
  font-size: 24rpx;
  color: #333333;
}

.people-time-tetx text{
  color: #FF0000;
  font-size: 24rpx;
}

.residue-time{
  font-size: 24rpx;
  color: #999999;
}

.go-spell{
  background: #FF0000;
  width: 120rpx;
  height: 60rpx;
  border-radius: 10rpx;
  margin-left: 30rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
}

.pinkong{
  text-align: center;
  padding-top: 50px;
}

.pinkong image{
  width: 452rpx;
  height: 368rpx;
}
.pinkong view{
  font-size: 34rpx;
  color: #666666;
  margin-top: 10rpx;
}